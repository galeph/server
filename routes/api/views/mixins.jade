//- Conditions HTML IEmixin increment (name, evs)	section.all-100.align-center.push-center( infinite-scroll=name + '()', infinite-scroll-distance=2 )		i.icon.icon-spinner.icon-pulse.icon-2x( ng-event=evs, ng-show="!noMore" )//- @method ie	@param condition {string}	@param not {boolean}	@return {comment}		+ie( 'lte IE 8', true !! false )		link(..)mixin ie (condition, not)	!="<!" + ( not == false || not === '' ?  '' : '--' ) + "[if " + condition + "]>"		block	!="<![endif]" + ( not == false || not === '' ?  '' : '--' ) + ">"//- @method ie-simple	@param condition {boolean|string}	@param version {number|null}	@param not {boolean}	@return {comment}		+ie-simple( 'lte', 8, true )		link(..)mixin ie-simple (condition, version, not)	!="<!" + ( not ?  '--' : '' )  + "[if "+ (typeof condition == 'boolean' ? condition ? '' : '!' : condition + ' ' ) + "IE " + ( typeof condition == 'string' && !version ? 6 : version || '' ) + "]>"		block	!="<![endif]" + ( not ?  '--' : '' ) + ">"//- Basic in a htmlmixin meta()	meta( charset="utf-8")	meta( http-equiv="X-UA-Compatible" content="IE=edge" )	meta( name="viewport" content="width=device-width, initial-scale=1" )	link( rel="shortcut icon", href="/favicon.ico")//- Templates in brower//- @method template	@param type {string}	@param name {string}	@return {script}		+template( 'ember', 'my-template')		p The templatemixin template( my, name )	script( type={		'angular'	: 'text/ng-template',		'knockout' 	: 'text/html',		'ember'		: 'text/x-handlebars' }[ my ],		id!=attributes.id || name ,		data-template-name!=attributes.id || name )		blockmixin GoogleAnalytics(UA)	| ga('create','#{UA}','auto');mixin constant(name, constant )	| [p[1]]('#{name}','#{constant}')